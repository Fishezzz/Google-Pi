{
    "manifest": {
        "displayName": "ME Pie",
        "invocationName": "ME Pie",
        "category": "PRODUCTIVITY"
    },
    "actions": [
        {
            "name": "com.example.actions.MAIN",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.example.intents.MAIN",
                "parameters": [
                    {
                        "name": "someValue",
                        "type": "SchemaOrg_Text"
                    }
                ],
                "trigger": {
                    "queryPatterns": [
                        "Hello $SchemaOrg_Text:someValue",
                        "Hi $SchemaOrg_Text:someValue",
                        "Hey $SchemaOrg_Text:someValue",
                        "Okay $SchemaOrg_Text:someValue"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "Hi, my name is not $someValue, but what can I do for you?"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.example.commands.MAIN",
                                    "params": {
                                        "someValue": "$someValue"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "name": "com.example.actions.SayRepeat",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.example.intents.SayRepeat",
                "parameters": [
                    {
                        "name": "text",
                        "type": "SchemaOrg_Text"
                    }
                ],
                "trigger": {
                    "queryPatterns": [
                        "Say $SchemaOrg_Text:text",
                        "Repeat $SchemaOrg_Text:text",
                        "Repeat me: $SchemaOrg_Text:text"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "$text"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.example.commands.SayRepeat",
                                    "params": {
                                        "text": "$text"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "name": "com.example.actions.MyDevices",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.example.intents.MyDevices",
                "parameters": [
                    {
                        "name": "status",
                        "type": "Status"
                    },
                    {
                        "name": "device",
                        "type": "Device"
                    }
                ],
                "trigger": {
                    "queryPatterns": [
                        "Turn $Status:status $Device:device",
                        "Turn $Device:device $Status:status"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "Turning $device $status"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.example.commands.MyDevices",
                                    "params": {
                                        "status": "$status",
                                        "device": "$device"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "name": "com.example.actions.BlinkLight",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.example.intents.BlinkLight",
                "parameters": [
                    {
                        "name": "number",
                        "type": "SchemaOrg_Number"
                    },
                    {
                        "name": "speed",
                        "type": "Speed"
                    }
                ],
                "trigger": {
                    "queryPatterns": [
                        "blink ($Speed:speed)? $SchemaOrg_Number:number times",
                        "blink $SchemaOrg_Number:number times ($Speed:speed)?",
                        "($Speed:speed)? blink $SchemaOrg_Number:number times"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "Blinking $speed $number times"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.example.commands.BlinkLight",
                                    "params": {
                                        "speed": "$speed",
                                        "number": "$number"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "name": "com.example.actions.LEDColor",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.example.intents.LEDColor",
                "parameters": [
                    {
                        "name": "color",
                        "type": "SchemaOrg_Color"
                    },
                    {
                        "name": "device",
                        "type": "Device"
                    }
                ],
                "trigger": {
                    "queryPatterns": [
                        "make $Device:device $SchemaOrg_Color:color"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "The color of $device is now $color"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.example.commands.LEDColor",
                                    "params": {
                                        "device": "$device",
                                        "color": "$color"
                                    }
                                }
                            }
                        ]
                    }
                }
            }  
        }
    ],
